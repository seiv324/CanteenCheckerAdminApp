<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.CanteenDetailActivity">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/canteenDetailNameTxtField"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Canteen Name" />

        <TextView
            android:id="@+id/canteenDetailAddressTxtField"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Canteen address" />

        <TextView
            android:id="@+id/canteenDetailPhoneNumberTxtField"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Phone Number" />

        <TextView
            android:id="@+id/canteenDetailWebsiteTxtField"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Website" />

        <TextView
            android:id="@+id/canteenDetailDishTxtField"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Dish Detail" />

        <TextView
            android:id="@+id/canteenDetailDishPriceTxtField"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Dish Price" />

        <TextView
            android:id="@+id/canteenDetailWaitingTimeTxtField"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Waiting Time" />

        <Button
            android:id="@+id/updateCanteenButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Button Canteen" />

        <Button
            android:id="@+id/updateDishButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Button Dish" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>